---
import { Image } from 'astro:assets'

export interface Props {
  cover: string
  coverAlt: string
}

const cx = {
  img: 'rounded-lg aspect-video object-cover mb-3 u-photo',
}

const { cover, coverAlt } = Astro.props
---

<Image alt={coverAlt} class={cx.img} src={cover} width={656} height={369} />
